{
  "name": "vigour-chromecast",
  "version": "1.0.1",
  "description": "Everything needed for Chromecast sender apps",
  "main": "lib/index.js",
  "scripts": {
    "start": "gaston",
    "test": "gaston test -r browser"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vigour-io/chromecast.git"
  },
  "keywords": [
    "vigour",
    "wrapper",
    "native",
    "plugin",
    "chromecast"
  ],
  "author": "Vigour",
  "contributors": [
    "Shawn Inder <shawn@vigour.io>",
    "Marcus Besjes <marcus@vigour.io>",
    "Alexander van der Werff <alex@vigour.io>",
    "Michiel van Liempt <michiel@vigour.io>",
    "Renan Carvalho <renan@vigour.io>",
    "Valerio Barrila <valerio@vigour.io>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vigour-io/chromecast/issues"
  },
  "homepage": "https://github.com/vigour-io/chromecast",
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "vigour-wrapper": "*",
    "vigour-ua": "*"
  },
  "devDependencies": {
    "bluebird": "^3.0.5",
    "vigour-element": "0.0.3",
    "vigour-uikit": "0.0.7"
  },
  "vigour": {
    "plugin": {
      "android": {
        "skipInstall": true,
        "className": "io.vigour.chromecast",
        "instantiation": "new Chromecast(this, webview)"
      }
    }
  },
  "standard": {
    "globals": [
      "window",
      "session",
      "chrome",
      "describe",
      "it",
      "expect",
      "before",
      "after",
      "beforeEach",
      "afterEach",
      "sinon"
    ]
  }
}
